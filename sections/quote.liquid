<section class="quote">
	<div class="page-width inner">
		<h2 style="color:{{ section.settings.color_title_label }}; font-size:{{ section.settings.font_size }}px;" class="quote__title">{{ section.settings.title-one }}</h2>
	</div>
</section>
  {% schema %}
  {
    "name": "quote",
    "tag": "section",
    "class": "section home page",
    "settings": [
      {
        "type": "checkbox",
        "id": "include_margins",
        "default": true,
        "label": "t:sections.apps.settings.include_margins.label"
      },
      {
          "type": "inline_richtext",
          "id": "title-one",
          "label": "title"
        },
        {
          "type": "color",
          "id": "color_title_label",
          "label": "Color Title Label",
          "default": "#383633"
        },
        {
          "type": "text",
          "id": "font_size",
          "label": "Font Size Enter Number"
        }
    ],
    "presets": [
      {
        "name": "quote",
        "blocks": [
       
        ]
      }
    ]
  }
  {% endschema %}
  