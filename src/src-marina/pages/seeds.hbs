				cloneItems.append( galleryButton );

				galleryButton.addEventListener('click', () => {
					const mainItem = mainItemEl.querySelector( '.gallery__item' );
					mainItem.innerHTML = galleryButton.innerHTML;
				});
			});

			parent.remove();
		}
	}
