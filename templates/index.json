        "71291754-f50f-4388-835f-a7cb9737039b": {
          "type": "single-review",
          "settings": {
            "photo": "shopify:\/\/shop_images\/Ellipse_4.jpg",
            "title-one": "Chris Hodges",
            "color_name": "#4d4d4e",
